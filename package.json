{
  "name": "tailwindcss-capsize",
  "version": "1.1.0",
  "description": "TailwindCSS utility classes for trimming whitespace above & below capital letters.",
  "keywords": [
    "tailwindcss",
    "tailwindcss-plugin",
    "capsize",
    "typography",
    "whitespace"
  ],
  "license": "ISC",
  "author": "Jeff Nelson <rustyangel@gmail.com> (http://tidaltheory.co/)",
  "main": "dist/index.js",
  "module": "dist/tailwindcss-capsize.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdx build",
    "changeset": "changeset add",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "release": "yarn build && changeset publish",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "capsize": "1.1.0",
    "tailwindcss": "1.6.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.2.7",
    "@changesets/cli": "2.10.x",
    "@navith/tailwindcss-plugin-author-types": "0.2.0",
    "@types/lodash.merge": "4.6.x",
    "@typescript-eslint/eslint-plugin": "3.9.x",
    "@typescript-eslint/parser": "3.9.x",
    "@zazen/eslint-config": "2.0.x",
    "eslint": "7.7.x",
    "husky": "4.2.x",
    "lint-staged": "10.4.x",
    "lodash.merge": "4.6.x",
    "prettier": "2.0.x",
    "prettier-plugin-packagejson": "2.2.x",
    "tsdx": "0.13.3",
    "tslib": "2.0.x",
    "typescript": "3.9.x"
  },
  "peerDependencies": {
    "tailwindcss": ">= 1.5"
  },
  "engines": {
    "node": ">=10"
  }
}
